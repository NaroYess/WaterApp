<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.naro.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Naro's Cool App</name>

    <description>
        An example for phonegap build docs. 
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        Naro Yess
    </author>

    <!-- Test -->

 	<feature name="SocialSharing">
      <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>



    <!-- Social Sharing -->
    <gap:plugin name="nl.x-services.plugins.socialsharing" source="plugins.cordova.io" />

    <!-- Local Notification -->
    <gap:plugin name="de.appplant.cordova.plugin.local-notification" version="0.8.1" />

     <!-- Screen shot -->
    <gap:plugin name="com.darktalker.cordova.screenshot" version="0.1.0" />


</widget>